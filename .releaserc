{
  "branches": [
    "main"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "NODE_ENV=production npm run gen-docs -- ./openapi.yaml",
        "publishCmd": "scalar registry publish ./openapi.yaml --version ${nextRelease.version}"
      }
    ]
  ]
}
